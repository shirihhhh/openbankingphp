{
  "name": "signalfire/woocommerce-truelayer-gateway",
  "description": "TrueLayer payments for Woocommerce",
  "type": "project",
  "license": "MIT",
  "authors": [
      {
          "name": "Robert Coster",
          "email": "rob@signalfire.co.uk"
      }
  ],
  "scripts": {
    "sniff": "./vendor/bin/phpcs --standard=WordPress --ignore=*/tests/*,*/vendor/* ./",
    "sniff:fix": "./vendor/bin/phpcbf --standard=WordPress --ignore=*/tests/*,*/vendor/* ./"        
  },
  "minimum-stability": "dev",
  "require": {
      "signalfire/truelayer-payments": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "dealerdirect/phpcodesniffer-composer-installer": "dev-master",
    "wp-coding-standards/wpcs": "dev-master"
  },
  "autoload": {
    "psr-4": {"Signalfire\\Woocommerce\\TrueLayer\\": "src/"}
  }  
}
